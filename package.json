  "scripts": {
    "ng": "ng",
    "start": "webpack-dev-server --port=4200",
    "build:dev": "webpack",
    "build:prod": "webpack --config webpack.prod.config.js",
    "build:server:prod": "webpack --config webpack.server.config.js",
    "build:universal:server": "webpack --config webpack.server-start.config.js --progress --colors",
    "test": "karma start ./karma.conf.js",
    "lint": "ng lint",
    "e2e": "protractor ./protractor.conf.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "build:universal:npm": "npm run build:prod && npm run build:server:prod --output-hashing=false && npm run build:universal:server",
    "build:ssr": "ng build --prod && ng build --prod --app 1 --output-hashing=false && npm run build:universal:server",
    "serve:ssr": "node dist/server.js",
    "build": "webpack",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
